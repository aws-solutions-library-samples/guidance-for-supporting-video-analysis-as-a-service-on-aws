    "components": {
        "schemas": {
            "AccessDeniedExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "ConflictExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "CreateLivestreamSessionRequestContent": {
                "type": "object",
                "properties": {
                    "deviceId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    },
                    "clientId": {
                        "type": "string",
                        "maxLength": 256,
                        "minLength": 1,
                        "pattern": "^(?!((A|a)(W|w)(S|s))_.*)[a-zA-Z0-9_.\\-]"
                    }
                },
                "required": [
                    "deviceId"
                ]
            },
            "CreateLivestreamSessionResponseContent": {
                "type": "object",
                "properties": {
                    "sessionId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-z0-9\\-]+$"
                    },
                    "clientId": {
                        "type": "string",
                        "maxLength": 256,
                        "minLength": 1,
                        "pattern": "^(?!((A|a)(W|w)(S|s))_.*)[a-zA-Z0-9_.\\-]"
                    },
                    "iceServers": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/IceServer"
                        }
                    },
                    "signalingChannelURL": {
                        "type": "string"
                    }
                }
            },
            "CreatePlaybackSessionRequestContent": {
                "type": "object",
                "properties": {
                    "deviceId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    },
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "endTime": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "required": [
                    "deviceId",
                    "endTime",
                    "startTime"
                ]
            },
            "CreatePlaybackSessionResponseContent": {
                "type": "object",
                "properties": {
                    "streamSources": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/StreamSource"
                        }
                    }
                }
            },
            "CreateSnapshotUploadPathRequestContent": {
                "type": "object",
                "properties": {
                    "deviceId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    },
                    "checksum": {
                        "type": "string"
                    },
                    "contentLength": {
                        "type": "number"
                    }
                },
                "required": [
                    "checksum",
                    "contentLength",
                    "deviceId"
                ]
            },
            "IceServer": {
                "type": "object",
                "properties": {
                    "password": {
                        "type": "string"
                    },
                    "ttl": {
                        "type": "number",
                        "format": "float"
                    },
                    "uris": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "maxLength": 100,
                            "minLength": 1
                        },
                        "maxItems": 100,
                        "minItems": 1,
                        "uniqueItems": true
                    },
                    "username": {
                        "type": "string"
                    }
                }
            },
            "InternalServerExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "PeerConnectionState": {
                "type": "string",
                "enum": [
                    "Pending",
                    "Connecting",
                    "Connected",
                    "Disconnected",
                    "Failed"
                ]
            },
            "ResourceNotFoundExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "SourceInfo": {
                "type": "object",
                "properties": {
                    "hLSStreamingURL": {
                        "type": "string"
                    },
                    "expirationTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "peerConnectionState": {
                        "$ref": "#/components/schemas/PeerConnectionState"
                    },
                    "SignalingChannelURL": {
                        "type": "string"
                    },
                    "clientId": {
                        "type": "string"
                    }
                }
            },
            "SourceType": {
                "type": "string",
                "enum": [
                    "HLS",
                    "WEBRTC"
                ]
            },
            "StartExportVideosRequestContent": {
                "type": "object",
                "properties": {
                    "startTime": {
                        "type": "number",
                        "format": "double"
                    },
                    "endTime": {
                        "type": "number",
                        "format": "double"
                    },
                    "device": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    },
                    "exportS3Path": {
                        "type": "string",
                        "maxLength": 2048,
                        "minLength": 1,
                        "pattern": "^s3://[a-z0-9][a-z0-9\\.-]{1,61}[a-z0-9](/.*)?$"
                    }
                },
                "required": [
                    "device",
                    "endTime",
                    "exportS3Path",
                    "startTime"
                ]
            },
            "StartExportVideosResponseContent": {
                "type": "object",
                "properties": {
                    "jobId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-z0-9\\-]+$"
                    }
                }
            },
            "StreamSource": {
                "type": "object",
                "properties": {
                    "sourceType": {
                        "$ref": "#/components/schemas/SourceType"
                    },
                    "source": {
                        "$ref": "#/components/schemas/SourceInfo"
                    },
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },
            "ValidationExceptionField": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message",
                    "name"
                ]
            },
            "ValidationExceptionReason": {
                "type": "string",
                "enum": [
                    "unknownOperation",
                    "cannotParse",
                    "fieldValidationFailed",
                    "other"
                ]
            },
            "ValidationExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "reason": {
                        "$ref": "#/components/schemas/ValidationExceptionReason"
                    },
                    "fieldList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationExceptionField"
                        }
                    }
                },
                "required": [
                    "message"
                ]
            }
        }
    }
}
