{
    "openapi": "3.1.0",
    "info": {
        "title": "Video Analytic Guidance Solution",
        "version": "2024-10-18"
    },
    "paths": {
        "/delete-device": {
            "post": {
                "operationId": "DeleteDevice",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DeleteDeviceRequestContent"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "DeleteDevice 200 response"
                    },
                    "400": {
                        "description": "ValidationException 400 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ValidationExceptionResponseContent"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "AccessDeniedException 403 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AccessDeniedExceptionResponseContent"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "ResourceNotFoundException 404 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResourceNotFoundExceptionResponseContent"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "InternalServerException 500 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/InternalServerExceptionResponseContent"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/get-create-device-status/{jobId}": {
            "get": {
                "operationId": "GetCreateDeviceStatus",
                "parameters": [
                    {
                        "name": "jobId",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "maxLength": 128,
                            "minLength": 1,
                            "pattern": "^[a-z0-9\\-]+$"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "GetCreateDeviceStatus 200 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GetCreateDeviceStatusResponseContent"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "ValidationException 400 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ValidationExceptionResponseContent"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "InternalServerException 500 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/InternalServerExceptionResponseContent"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "AccessDeniedExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "DeleteDeviceRequestContent": {
                "type": "object",
                "properties": {
                    "deviceId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    }
                },
                "required": [
                    "deviceId"
                ]
            },
            "GetCreateDeviceStatusResponseContent": {
                "type": "object",
                "properties": {
                    "jobId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-z0-9\\-]+$"
                    },
                    "deviceId": {
                        "type": "string",
                        "maxLength": 128,
                        "minLength": 1,
                        "pattern": "^[a-zA-Z0-9:_\\-]+$"
                    },
                    "status": {
                        "$ref": "#/components/schemas/Status"
                    },
                    "createTime": {
                        "type": "number",
                        "format": "double"
                    },
                    "modifiedTime": {
                        "type": "number",
                        "format": "double"
                    },
                    "errorMessage": {
                        "type": "string"
                    }
                }
            },
            "InternalServerExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "ResourceNotFoundExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message"
                ]
            },
            "Status": {
                "type": "string",
                "enum": [
                    "RUNNING",
                    "COMPLETED",
                    "FAILED",
                    "CANCELLED"
                ]
            },
            "ValidationExceptionField": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "message",
                    "name"
                ]
            },
            "ValidationExceptionReason": {
                "type": "string",
                "enum": [
                    "unknownOperation",
                    "cannotParse",
                    "fieldValidationFailed",
                    "other"
                ]
            },
            "ValidationExceptionResponseContent": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "reason": {
                        "$ref": "#/components/schemas/ValidationExceptionReason"
                    },
                    "fieldList": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationExceptionField"
                        }
                    }
                },
                "required": [
                    "message",
                    "reason"
                ]
            }
        }
    }
}
